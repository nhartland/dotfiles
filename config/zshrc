# Path to oh-my-zsh 
fpath=(~/.zsh $fpath)
export ZSH=$HOME/.oh-my-zsh

## Plugins via oh-my-zsh
plugins=(cp osx)
#Look in ~/.oh-my-zsh/themes/
ZSH_THEME="honukai"
#Timestamp format
HIST_STAMPS="dd.mm.yyyy"

source $ZSH/oh-my-zsh.sh
source ~/.shellrc

# >>> conda initialize >>>
# !! Contents within this block are managed by 'conda init' !!
__conda_setup="$('/Users/hartland/miniconda3/bin/conda' 'shell.zsh' 'hook' 2> /dev/null)"
if [ $? -eq 0 ]; then
    eval "$__conda_setup"
else
    if [ -f "/Users/hartland/miniconda3/etc/profile.d/conda.sh" ]; then
        . "/Users/hartland/miniconda3/etc/profile.d/conda.sh"
    else
        export PATH="/Users/hartland/miniconda3/bin:$PATH"
    fi
fi
unset __conda_setup
# <<< conda initialize <<<

